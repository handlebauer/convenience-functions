{
	"compilerOptions": {
		"checkJs": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"target": "esnext",
		"module": "esnext",
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "lib",
		"noEmit": false,
		"moduleResolution": "nodenext"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
